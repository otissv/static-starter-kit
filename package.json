{
  "name": "static-starter-kit",
  "version": "1.0.0",
  "description": "Staic Web start kit",
  "main": "./server.js",
  "scripts": {
    "start": "node ./server.js",
    "css:build": "postcssbuild --config postcssbuild.js",
    "css:watch": "./src/styles",
    "flow": "flow check; [[ $?=0||$?=2 ]]",
    "flow:watch": "watch 'npm run flow' ./src",
    "prepublish": "npm run webpack:build && npm run css:build",
    "tape": "babel-node test-runner.js | tap-min  && date || true && date",
    "tape:spec": "babel-node test-runner.js | tap-difflet || true && date",
    "test": "npm run tape",
    "test:spec": "npm run tape:spec",
    "test:watch": "watch 'npm run tape' ./",
    "views:build": "node handlebarsbuild.js",
    "views": "node handlebarsbuild.js -w",
    "webpack": "webpack",
    "webpack:build": "webpack --config ./webpack.config-build.js --production --optimize-minimize",
    "webpack:watch": "webpack --watch"
  },
  "author": "Otis Virginie",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.1",
    "browser-sync": "^2.13.0",
    "cheerio": "^0.20.0",
    "cssnano": "^3.7.1",
    "eslint": "^3.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-smells": "^1.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "faucet": "0.0.1",
    "flow-bin": "^0.27.0",
    "flow-loader": "^1.0.2",
    "fs-watch-tree": "^0.2.5",
    "glob": "^7.0.5",
    "handlebars": "^4.0.5",
    "history": "^3.0.0",
    "json-loader": "^0.5.4",
    "postcss-build": "^1.1.11",
    "ramda": "^0.21.0",
    "tap-difflet": "^0.6.0",
    "tap-min": "^1.1.0",
    "tape": "^4.6.0",
    "uglify-loader": "^1.3.0",
    "watch": "^0.19.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  }
}
